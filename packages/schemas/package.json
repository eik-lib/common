    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "@babel/eslint-parser": "7.16.5",
    "prettier": "2.5.1",
    "tap": "15.2.3",
    "typescript": "4.7.4"
